<div class="row">
  <h3><%= form.label :email_address %></h3>
  <p><%= form.text_field :email_address %></p>
</div>
<div class="row">
  <h3><%= form.label :home_zip_code, "ZIP Code" %></h3>
  <p><%= form.text_field :home_zip_code %></p>
</div>
<div class="row">
  <h3><%= form.label :date_of_birth %></h3>
  <p><%= form.text_field :date_of_birth %></p>
</div>
<div class="row">
  <h3><%= form.check_box :us_citizen %> <%= form.label :us_citizen, "I am a U.S. citizen" %></h3>
</div>
<div class="row">
  <h3><%= form.check_box :first_registration %> <%= form.label :first_registration, "I am registering to vote for the the first time" %></h3>
</div>
